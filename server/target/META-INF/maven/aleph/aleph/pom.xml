  <version>0.4.4</version>
  <name>aleph</name>
  <description>a framework for asynchronous communication</description>
  <licenses>
    <license>
      <name>MIT License</name>
    </license>
  </licenses>
  <scm>
    <connection>scm:git:git://github.com/ztellman/aleph.git</connection>
    <developerConnection>scm:git:ssh://git@github.com/ztellman/aleph.git</developerConnection>
    <tag>66d64fe56059ea79dc614cf7ed2455f3582f4efb
</tag>
    <url>https://github.com/ztellman/aleph</url>
  </scm>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <testSourceDirectory>test</testSourceDirectory>
    <resources>
      <resource>
        <directory>resources</directory>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>resources</directory>
      </testResource>
    </testResources>
    <directory>target</directory>
    <outputDirectory>target/classes</outputDirectory>
    <plugins/>
